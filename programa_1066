#include <stdio.h>

int main()
{
    
    int x, i, impar = 0, par = 0, neg = 0, pos = 0;

    for (i = 0; i < 5; i++)
    {
    scanf("%d", &x);
    
    if (x%2 != 0)
      {
      impar++; 
      }
    if (x < 0)
      {
      neg++; 
      }
    if (x > 0)
      {
      pos++; 
      }
    if (x%2 == 0)
      {
      par++; 
      }
    }

    printf("%d valor(es) par(es)\n", par);
    printf("%d valor(es) impar(es)\n", impar);
    printf("%d valor(es) positivo(s)\n", pos);
    printf("%d valor(es) negativo(s)\n", neg);
  
  return 0;    
}
