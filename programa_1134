#include <stdio.h>

int main() 
{
       	int n, a=0, g=0, d=0;
        printf("MUITO OBRIGADO\n");
       
        while (n != 4) 
		{
         
            scanf("%d", &n);
            if (n>0 && n<4)
			if (n == 1) a++;
			else if (n == 2) g++;
			else d++;
        }
        printf("Alcool: %d\n", a);
        printf("Gasolina: %d\n", g);
        printf("Diesel: %d\n", d);

    return 0;
}
