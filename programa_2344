#include "stdio.h"

int main()
{
    int a;
    
    scanf("%d", &a);

    if(a <= 100 && a >= 0)
	{
      	if (a == 0)
			{
            printf("E\n");
			}
      	else if (a > 0 && a <= 35)
			{
            printf("D\n");
			}
       	else if (a > 35 && a <= 60)
			{
            printf("C\n");
			}	
      	else if (a > 60 && a <= 85)
			{
            printf("B\n");
			}
      	else if (a > 85)
			{
            printf("A\n");
			}		
	}
    return 0;			
}
