#include <stdio.h>
 
int main ()
{
    int inicio,final,duracao;

    scanf ("%d", &inicio);
    scanf ("%d", &final);

   	duracao=final-inicio;
   	
   	if (duracao<0)
   		{
		duracao=24+(final-inicio);	
		}
   	
    if (inicio==final)
    	{
        printf ("O JOGO DUROU 24 HORA(S)\n");
    	}
    else 
		{	
    	printf ("O JOGO DUROU %d HORA(S)\n", duracao);
		}
    
    return 0 ;
}
