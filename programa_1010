#include <stdio.h>

int main() {
	int codigo1, nPecas1, codigo2, nPecas2;
	float preco1, preco2, total; 
	
	scanf("%d%d%f",&codigo1,&nPecas1,&preco1);
    scanf("%d%d%f",&codigo2,&nPecas2,&preco2);
    
	total = nPecas1*preco1+nPecas2*preco2;
	
	printf("VALOR A PAGAR: R$ %.2f\n", total);
	
    return 0;
}
